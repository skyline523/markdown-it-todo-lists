{
  "name": "markdown-it-todo-lists",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.5.0",
  "description": "A markdown-it plugin to create todo lists",
  "author": "Leet <programingwithli@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/antfu/markdown-it-todo-list#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/markdown-it-todo-list.git"
  },
  "bugs": "https://github.com/antfu/markdown-it-todo-list/issues",
  "keywords": [
    "markdown-it",
    "todo",
    "task",
    "list",
    "checkbox"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "markdown-it": "^14.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@antfu/ni": "^0.22.0",
    "@antfu/utils": "^0.7.10",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.14.11",
    "bumpp": "^9.4.1",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "markdown-it": "^14.1.0",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
